        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Kedro Run",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "kedro",
            "cwd": "${workspaceFolder}/evaluate-one",
            "justMyCode": false,
            "args": [
                "run"
            ]
            // Any other arguments should be passed as a comma-seperated-list
            // e.g "args": ["run", "--pipeline", "pipeline_name"]
        },
        {
            "name": "Python: Kedro Node",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "kedro",
            "cwd": "${workspaceFolder}/evaluate-one",
            "justMyCode": false,
            "args": [
                "run",
                "--pipeline",
                "stepzz_evaluate_one",
                // "--nodes",
                // "transform_questions_2_dataframe "
            ]
            // Any other arguments should be passed as a comma-seperated-list
            // e.g "args": ["run", "--pipeline", "pipeline_name"]
        }
    ]
